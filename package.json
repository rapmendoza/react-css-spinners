{
  "name": "react-css-spinners",
  "version": "1.0.0",
  "description": "A CSS-only spinners for React",
  "main": "index.js",
  "scripts": {
    "format": "prettier-eslint --write '**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
